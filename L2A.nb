 RowBox[{"ClearAll", "[", 
  RowBox[{"dens", ",", "calcChFunc", ",", "chFunc"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"calcChFunc", "[", "\[Rho]_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "x", ",", "t", ",", "charfun", ",", "momfun", ",", "momt1", ",", 
       "momt2"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"charfun", "[", "t_", "]"}], "=", 
       RowBox[{"FourierTransform", "[", 
        RowBox[{
         RowBox[{"\[Rho]", "[", "x", "]"}], ",", "x", ",", "t", ",", 
         RowBox[{"FourierParameters", "->", 
          RowBox[{"{", 
           RowBox[{"1", ",", "1"}], "}"}]}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"momt1", "=", 
       RowBox[{"Limit", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"charfun", "'"}], "[", "t", "]"}], "*", 
          RowBox[{"(", 
           RowBox[{"-", "\[ImaginaryI]"}], ")"}]}], ",", 
         RowBox[{"t", "->", "0"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"momt2", " ", "=", " ", 
       RowBox[{"Limit", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"charfun", "''"}], "[", "t", "]"}], "*", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"-", "\[ImaginaryI]"}], ")"}], "^", "2"}]}], ",", 
         RowBox[{"t", "->", "0"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Med: \>\"", ",", "momt1"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Var: \>\"", ",", 
        RowBox[{"Simplify", "[", 
         RowBox[{"momt2", "-", 
          RowBox[{"momt1", "^", "2"}]}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Func. caracter\[IAcute]stica: \>\"", ",", 
        RowBox[{"Simplify", "@", 
         RowBox[{"charfun", "[", "t", "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"momfun", "[", "t_", "]"}], "=", 
       RowBox[{"charfun", "[", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "t"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Func. momento: \>\"", ",", 
        RowBox[{"FullSimplify", "@", " ", 
         RowBox[{"momfun", "[", "t", "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{
       "\"\<S\[EAcute]rie \!\(\*SubscriptBox[\(\[Psi]\), \(X\)]\)(t): \>\"", 
        ",", 
